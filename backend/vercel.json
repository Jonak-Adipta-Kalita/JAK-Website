{
    "builds": [{
        "src": "main_website/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": {
			"maxLambdaSize": "15mb",
			"runtime": "python3.9"
		}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main_website/wsgi.py"
        }
    ]
}
